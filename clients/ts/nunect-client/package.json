{
  "name": "@nunect/nunect-client",
  "version": "0.1.0",
  "description": "TypeScript client for nunect NATS Data Backend",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=browser --format=esm --outfile=dist/index.esm.js && esbuild src/index.ts --bundle --platform=browser --format=cjs --outfile=dist/index.js",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "test": "node --test tests/*.test.js",
    "lint": "echo 'No linter configured'",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "nats",
    "nunect",
    "messaging",
    "websocket",
    "iot"
  ],
  "author": "novamarc",
  "license": "MIT",
  "dependencies": {
    "nats.ws": "^1.29.2"
  },
  "devDependencies": {
    "esbuild": "^0.20.0",
    "typescript": "^5.3.0"
  }
}
